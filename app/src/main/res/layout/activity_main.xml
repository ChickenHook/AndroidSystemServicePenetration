<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/package_manager_attack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PackageManagerCrash" />
    <Button
        android:id="@+id/clipboard_manager_crash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ClipboardManagerCrash" />

    <Button
        android:id="@+id/service_manager_crash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ServiceManagerCrash" />

</androidx.appcompat.widget.LinearLayoutCompat>